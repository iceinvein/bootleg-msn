		"@types/react": "^19.1.12",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.2",
		"convex-test": "^0.0.38",
		"dotenv": "^17.2.2",
		"globals": "^16.3.0",
